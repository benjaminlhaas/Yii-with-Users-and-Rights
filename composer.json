{
    "name": "cg/yii-base-build",
    "description": "CG Yii Build",
    "keywords": ["package", "dependency", "autoload"],
    "homepage": "http://controlgroup.com/",
    "type": "library",
    "license": "BSD",
    "config": {
        "vendor-dir": "webapp/protected/vendors"
    }, 
    "repositories": [
        {
          "type": "vcs",
          "url": "http://github.com/evan108108/UnitTestYiis.git"
        },{
          "type": "vcs",
          "url": "http://github.com/evan108108/RESTFullYii.git"
        },{
          "type": "package",
          "package": {
            "name": "crisu83/bootstrap",
            "version": "master",
            "source": {
              "url": "https://bitbucket.org/Crisu83/yii-bootstrap",
              "type": "hg",
              "reference": "tip"
            }
          }
        },{
          "type": "package",
          "package": {
            "name": "idlesign/file",
            "version": "master",
            "source": {
              "url": "http://github.com/idlesign/ist-yii-cfile",
              "type": "git",
              "reference": "origin/master"
            }
          }
        },{
          "type": "package",
          "package": {
            "name": "generator4fixtures/gii",
            "version": "master",
            "source": {
              "url": "https://bitbucket.org/denis_medved/my-yii-extension",
              "type": "hg",
              "reference": "tip"
            }
          }
        },{
          "type": "package",
          "package": {
            "name": "weavora/wunit",
            "version": "master",
            "source": {
              "url": "https://github.com/weavora/wunit.git",
              "type": "git",
              "reference":"origin/master"
            }
          }
        }
    ],
     "require": {
        "php": ">=5.3.0",
        "cg/unit-test-yiis": "cg-dev",
        "cg/RESTFullYii": "cg-dev",
        "crisu83/bootstrap": "master",
        "generator4fixtures/gii": "master",
        "idlesign/file": "master",
        "weavora/wunit": "master"       
    }
}
